{
    "name": "akkurateio/laravel-carbon-icons",
    "description": "Icons package for Laravel applications based on IBM Carbon compatibility",
    "type": "library",
    "license": "AGPL-3.0-only",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Akkurate",
            "email": "hello@subvitamine.com",
            "homepage": "https://www.akkurate.io",
            "role": "Company"
        }
    ],
    "support": {
        "docs": "https://github.com/akkurateio/documentation"
    },
    "require": {
        "spatie/laravel-image-optimizer": "^1.6",
        "ext-dom": "*",
        "akkurateio/laravel-back-components": "0.x-dev",
        "akkurateio/laravel-core": "0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0"
    },
    "autoload" : {
        "psr-4": {
            "Akkurate\\LaravelCarbonIcons\\": "src"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Akkurate\\LaravelCarbonIcons\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akkurate\\LaravelCarbonIcons\\LaravelCarbonIconsServiceProvider"
            ]
        }
    }
}
